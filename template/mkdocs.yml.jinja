---
site_name: "{{project_name}}"
theme:
  name: material
  # palette:
  #  scheme: slate
  #  primary: cyan
  features:
    - content.tabs.link

# for adding extra css/js see https://www.mkdocs.org/user-guide/customizing-your-theme/

plugins:
  - search
  - mknotebooks:
      execute: false
  - mermaid2:
      version: 11.4.1
  
#watch:
#  - src/{{ project_slug }}/schema

nav:
  - Home: index.md
  
exclude_docs: |
  /templates-linkml/

site_url: https://{{github_org}}.github.io/{{project_name}}
repo_url: https://github.com/{{github_org}}/{{project_name}}
